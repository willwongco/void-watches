<style>
  @font-face {
    font-family: "font-futura";
    src: url({{ 'font-FuturaMedium.woff' | asset_url }});
    src: url({{ 'font-FuturaMedium.woff2' | asset_url }}) format('woff2'),
        url({{ 'font-FuturaMedium.woff' | asset_url }}) format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
  }

  @font-face {
    font-family: "font-futura";
    src: url({{ 'font-FuturaMedium.woff' | asset_url }});
    src: url({{ 'font-FuturaMedium.woff2' | asset_url }}) format('woff2'),
        url({{ 'font-FuturaMedium.woff' | asset_url }}) format('woff');
    font-weight: 700;
    font-style: normal;
    font-display: swap;
  }
</style>


{% comment %} {%- unless settings.type_body_font.system?
  and settings.type_subheading_font.system?
  and settings.type_heading_font.system?
-%}
  <link
    rel="preconnect"
    href="https://fonts.shopifycdn.com"
    crossorigin
  >
{%- endunless -%}

{% # theme-check-disable %}
{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
  >
{%- endunless -%}
{%- unless settings.type_subheading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_subheading_font | font_url }}"
    type="font/woff2"
    crossorigin
  >
{%- endunless -%}
{%- unless settings.type_heading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_heading_font | font_url }}"
    type="font/woff2"
    crossorigin
  >
{%- endunless -%}
{%- unless settings.type_accent_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_accent_font | font_url }}"
    type="font/woff2"
    crossorigin
  >
{%- endunless -%}
{% # theme-check-enable %} {% endcomment %}

