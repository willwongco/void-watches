{% doc %}
  @prompt
    Create a comparison chart with 6 columns. At the top of each column should be an image. Below the image a text line and a call to action button. Below this there should be 4-6 lines of text with a description at the very left.
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-comparison-chart-{{ ai_gen_id }} {
    width: 100%;
    max-width: 100%;
    overflow-x: auto;
    padding: {{ block.settings.section_padding }}px 0;
  }

  .ai-comparison-chart__container-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(6, 1fr);
    gap: {{ block.settings.column_gap }}px;
    min-width: 800px;
    padding: 0 20px;
  }

  .ai-comparison-chart__column-{{ ai_gen_id }} {
    background-color: {{ block.settings.column_background_color }};
    border: {{ block.settings.column_border_width }}px solid {{ block.settings.column_border_color }};
    border-radius: {{ block.settings.column_border_radius }}px;
    padding: {{ block.settings.column_padding }}px;
    text-align: center;
    display: flex;
    flex-direction: column;
    height: 100%;
  }

  .ai-comparison-chart__image-wrapper-{{ ai_gen_id }} {
    width: 100%;
    height: {{ block.settings.image_height }}px;
    margin-bottom: {{ block.settings.content_spacing }}px;
    border-radius: {{ block.settings.image_border_radius }}px;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f5f5f5;
  }

  .ai-comparison-chart__image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-comparison-chart__image-wrapper-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-comparison-chart__title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_font_size }}px;
    font-weight: 600;
    color: {{ block.settings.title_color }};
    margin-bottom: {{ block.settings.content_spacing }}px;
    line-height: 1.3;
  }

  .ai-comparison-chart__button-{{ ai_gen_id }} {
    background-color: {{ block.settings.button_background_color }};
    color: {{ block.settings.button_text_color }};
    border: {{ block.settings.button_border_width }}px solid {{ block.settings.button_border_color }};
    border-radius: {{ block.settings.button_border_radius }}px;
    padding: {{ block.settings.button_padding_vertical }}px {{ block.settings.button_padding_horizontal }}px;
    font-size: {{ block.settings.button_font_size }}px;
    font-weight: 600;
    text-decoration: none;
    display: inline-block;
    transition: all 0.3s ease;
    margin-bottom: {{ block.settings.content_spacing }}px;
    cursor: pointer;
  }

  .ai-comparison-chart__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_background_color }};
    color: {{ block.settings.button_hover_text_color }};
    border-color: {{ block.settings.button_hover_border_color }};
  }

  .ai-comparison-chart__features-{{ ai_gen_id }} {
    flex-grow: 1;
    display: flex;
    flex-direction: column;
    gap: {{ block.settings.feature_spacing }}px;
  }

  .ai-comparison-chart__feature-{{ ai_gen_id }} {
    display: flex;
    align-items: flex-start;
    text-align: left;
    gap: 12px;
  }

  .ai-comparison-chart__feature-label-{{ ai_gen_id }} {
    font-size: {{ block.settings.feature_label_font_size }}px;
    font-weight: 600;
    color: {{ block.settings.feature_label_color }};
    min-width: 120px;
    flex-shrink: 0;
  }

  .ai-comparison-chart__feature-value-{{ ai_gen_id }} {
    font-size: {{ block.settings.feature_value_font_size }}px;
    color: {{ block.settings.feature_value_color }};
    line-height: 1.4;
    flex-grow: 1;
  }

  @media screen and (max-width: 1024px) {
    .ai-comparison-chart__container-{{ ai_gen_id }} {
      grid-template-columns: repeat(3, 1fr);
      min-width: 600px;
    }
  }

  @media screen and (max-width: 768px) {
    .ai-comparison-chart__container-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);
      min-width: 400px;
      gap: {{ block.settings.column_gap | divided_by: 2 }}px;
    }

    .ai-comparison-chart__column-{{ ai_gen_id }} {
      padding: {{ block.settings.column_padding | times: 0.8 }}px;
    }

    .ai-comparison-chart__image-wrapper-{{ ai_gen_id }} {
      height: {{ block.settings.image_height | times: 0.8 }}px;
    }

    .ai-comparison-chart__feature-{{ ai_gen_id }} {
      flex-direction: column;
      gap: 4px;
    }

    .ai-comparison-chart__feature-label-{{ ai_gen_id }} {
      min-width: auto;
    }
  }

  @media screen and (max-width: 480px) {
    .ai-comparison-chart__container-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      min-width: auto;
    }
  }
{% endstyle %}

<div class="ai-comparison-chart-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-comparison-chart__container-{{ ai_gen_id }}">
    {% for i in (1..6) %}
      {% liquid
        assign image_key = 'column_' | append: i | append: '_image'
        assign title_key = 'column_' | append: i | append: '_title'
        assign button_text_key = 'column_' | append: i | append: '_button_text'
        assign button_url_key = 'column_' | append: i | append: '_button_url'
        assign feature_1_label_key = 'column_' | append: i | append: '_feature_1_label'
        assign feature_1_value_key = 'column_' | append: i | append: '_feature_1_value'
        assign feature_2_label_key = 'column_' | append: i | append: '_feature_2_label'
        assign feature_2_value_key = 'column_' | append: i | append: '_feature_2_value'
        assign feature_3_label_key = 'column_' | append: i | append: '_feature_3_label'
        assign feature_3_value_key = 'column_' | append: i | append: '_feature_3_value'
        assign feature_4_label_key = 'column_' | append: i | append: '_feature_4_label'
        assign feature_4_value_key = 'column_' | append: i | append: '_feature_4_value'
        assign feature_5_label_key = 'column_' | append: i | append: '_feature_5_label'
        assign feature_5_value_key = 'column_' | append: i | append: '_feature_5_value'
        assign feature_6_label_key = 'column_' | append: i | append: '_feature_6_label'
        assign feature_6_value_key = 'column_' | append: i | append: '_feature_6_value'

        assign column_image = block.settings[image_key]
        assign column_title = block.settings[title_key]
        assign button_text = block.settings[button_text_key]
        assign button_url = block.settings[button_url_key]
        assign feature_1_label = block.settings[feature_1_label_key]
        assign feature_1_value = block.settings[feature_1_value_key]
        assign feature_2_label = block.settings[feature_2_label_key]
        assign feature_2_value = block.settings[feature_2_value_key]
        assign feature_3_label = block.settings[feature_3_label_key]
        assign feature_3_value = block.settings[feature_3_value_key]
        assign feature_4_label = block.settings[feature_4_label_key]
        assign feature_4_value = block.settings[feature_4_value_key]
        assign feature_5_label = block.settings[feature_5_label_key]
        assign feature_5_value = block.settings[feature_5_value_key]
        assign feature_6_label = block.settings[feature_6_label_key]
        assign feature_6_value = block.settings[feature_6_value_key]
      %}

      <div class="ai-comparison-chart__column-{{ ai_gen_id }}">
        <div class="ai-comparison-chart__image-wrapper-{{ ai_gen_id }}">
          {% if column_image %}
            <img
              src="{{ column_image | image_url: width: 400 }}"
              alt="{{ column_image.alt | escape }}"
              class="ai-comparison-chart__image-{{ ai_gen_id }}"
              loading="lazy"
              width="{{ column_image.width }}"
              height="{{ column_image.height }}"
            >
          {% else %}
            {{ 'image' | placeholder_svg_tag }}
          {% endif %}
        </div>

        {% if column_title != blank %}
          <h3 class="ai-comparison-chart__title-{{ ai_gen_id }}">{{ column_title }}</h3>
        {% endif %}

        {% if button_text != blank %}
          <a
            href="{{ button_url | default: '#' }}"
            class="ai-comparison-chart__button-{{ ai_gen_id }}"
          >
            {{ button_text }}
          </a>
        {% endif %}

        <div class="ai-comparison-chart__features-{{ ai_gen_id }}">
          {% if feature_1_label != blank or feature_1_value != blank %}
            <div class="ai-comparison-chart__feature-{{ ai_gen_id }}">
              <div class="ai-comparison-chart__feature-label-{{ ai_gen_id }}">{{ feature_1_label }}</div>
              <div class="ai-comparison-chart__feature-value-{{ ai_gen_id }}">{{ feature_1_value }}</div>
            </div>
          {% endif %}

          {% if feature_2_label != blank or feature_2_value != blank %}
            <div class="ai-comparison-chart__feature-{{ ai_gen_id }}">
              <div class="ai-comparison-chart__feature-label-{{ ai_gen_id }}">{{ feature_2_label }}</div>
              <div class="ai-comparison-chart__feature-value-{{ ai_gen_id }}">{{ feature_2_value }}</div>
            </div>
          {% endif %}

          {% if feature_3_label != blank or feature_3_value != blank %}
            <div class="ai-comparison-chart__feature-{{ ai_gen_id }}">
              <div class="ai-comparison-chart__feature-label-{{ ai_gen_id }}">{{ feature_3_label }}</div>
              <div class="ai-comparison-chart__feature-value-{{ ai_gen_id }}">{{ feature_3_value }}</div>
            </div>
          {% endif %}

          {% if feature_4_label != blank or feature_4_value != blank %}
            <div class="ai-comparison-chart__feature-{{ ai_gen_id }}">
              <div class="ai-comparison-chart__feature-label-{{ ai_gen_id }}">{{ feature_4_label }}</div>
              <div class="ai-comparison-chart__feature-value-{{ ai_gen_id }}">{{ feature_4_value }}</div>
            </div>
          {% endif %}

          {% if feature_5_label != blank or feature_5_value != blank %}
            <div class="ai-comparison-chart__feature-{{ ai_gen_id }}">
              <div class="ai-comparison-chart__feature-label-{{ ai_gen_id }}">{{ feature_5_label }}</div>
              <div class="ai-comparison-chart__feature-value-{{ ai_gen_id }}">{{ feature_5_value }}</div>
            </div>
          {% endif %}

          {% if feature_6_label != blank or feature_6_value != blank %}
            <div class="ai-comparison-chart__feature-{{ ai_gen_id }}">
              <div class="ai-comparison-chart__feature-label-{{ ai_gen_id }}">{{ feature_6_label }}</div>
              <div class="ai-comparison-chart__feature-value-{{ ai_gen_id }}">{{ feature_6_value }}</div>
            </div>
          {% endif %}
        </div>
      </div>
    {% endfor %}
  </div>
</div>

